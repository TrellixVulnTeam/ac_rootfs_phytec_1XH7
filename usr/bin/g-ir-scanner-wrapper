#!/bin/sh
# This prevents g-ir-scanner from writing cache data to /var/lib/jenkins
export GI_SCANNER_DISABLE_CACHE=1

g-ir-scanner --use-binary-wrapper=/media/jenkins1/builddata1/yocto_ti_pd18.2.0/build_am5728/arago-tmp-external-linaro-toolchain/sysroots/am5728-phycore-rdk/usr/bin/g-ir-scanner-qemuwrapper --use-ldd-wrapper=/media/jenkins1/builddata1/yocto_ti_pd18.2.0/build_am5728/arago-tmp-external-linaro-toolchain/sysroots/am5728-phycore-rdk/usr/bin/g-ir-scanner-lddwrapper --add-include-path=/media/jenkins1/builddata1/yocto_ti_pd18.2.0/build_am5728/arago-tmp-external-linaro-toolchain/sysroots/am5728-phycore-rdk/usr/share/gir-1.0 "$@"
